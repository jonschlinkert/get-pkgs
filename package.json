{
  "name": "get-pkgs",
  "description": "Get the package.json for an array of repos from the npm registry, optionally filtering properties using glob patterns.",
  "version": "0.5.0",
  "homepage": "https://github.com/jonschlinkert/get-pkgs",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/get-pkgs",
  "bugs": {
    "url": "https://github.com/jonschlinkert/get-pkgs/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-yellow": "^0.1.1",
    "async-array-reduce": "^0.1.0",
    "get-pkg": "^0.3.0",
    "lazy-cache": "^1.0.3",
    "warning-symbol": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5",
    "should": "^8.3.0"
  },
  "keywords": [
    "get",
    "json",
    "module",
    "npm",
    "object",
    "package",
    "package.json",
    "pkg",
    "pkgs"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "get-first-commit",
        "get-pkg",
        "github-base"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
